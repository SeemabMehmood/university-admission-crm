<% if params[:action_name] == "add" %>
  $("#icon-assigned-<%= @representing_institution.id %>").html("<i class='fa fa-check-circle-o text-success'></i>");
<% elsif params[:action_name] == "remove" %>
  $("#icon-assigned-<%= @representing_institution.id %>").html("");
<% end %>
$("#manage_links-<%= @representing_institution.id %>").html("<%= j render('representing_institutions/links_counsellor') %>");

toastr['error']('<%= @message %>');
